{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "hexo generate && node scripts/html-formatter.js",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "multi-deploy": "node scripts/multi-deploy.js",
    "setup-multi-platform": "node scripts/setup-multi-platform.js",
    "test-build": "node scripts/test-build.js",
    "build:cloudflare": "npx hexo generate --config _config.cloudflare.yml",
    "server": "hexo server",
    "setup": "node scripts/setup.js",
    "dev": "hexo clean && hexo server",
    "preview": "hexo clean && hexo generate && node scripts/html-formatter.js && hexo server",
    "check": "node scripts/check-config.js",
    "format-html": "node scripts/html-formatter.js",
    "update-theme": "node scripts/update-theme.js",
    "check-theme-update": "node scripts/update-theme.js --check"
  },
  "hexo": {
    "version": "7.3.0"
  },
  "dependencies": {
    "hexo": "^7.3.0",
    "hexo-deployer-git": "^4.0.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^4.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^7.0.0",
    "hexo-renderer-pug": "^3.0.0",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^1.0.0",
    "js-yaml": "^4.1.0"
  }
}
